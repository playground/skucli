{
  "name": "skucli",
  "version": "0.1.1",
  "description": "",
  "main": "./build/index.js",
  "bin": {
    "skucli": "./build/index.js"
  },
  "scripts": {
    "start": "nodemon -e ts --watch 'src/**/*.ts' --exec 'npm run build'",
    "build": "npx tsc && chmod +x build/*.js && chmod +x build/**/*.js",
    "build:watch": "onchange 'src/**/*.ts' -- npm run build",
    "local": "npm run build && npm i -g && skucli",
    "ibm-login": "ibmcloud login --sso && npm run select-target && npm run select-project && npm run switch-region",
    "select-project": "ibmcloud ce project select -n ieam",
    "select-target": "ibmcloud target -g Default",
    "switch-region": "ibmcloud cr region-set ${region} && ibmcloud cr login",
    "postinstall": "ts-node bin/install.ts",
    "pre-commit": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playground/skucli.git"
  },
  "author": "makadodo@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/playground/skucli/issues"
  },
  "homepage": "https://github.com/playground/skucli#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.2",
    "@types/source-map-support": "^0.5.6",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/parser": "^5.59.5",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "onchange": "^7.1.0",
    "serve": "^14.2.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^10.0.1",
    "figlet": "^1.6.0",
    "jsonfile": "^6.1.0",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "yargs": "^17.7.2"
  }
}
